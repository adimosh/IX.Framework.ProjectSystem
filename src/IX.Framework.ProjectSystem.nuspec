<?xml version="1.0"?>
<package >
    <metadata minClientVersion="4.0">
    <id>IX.Framework.ProjectSystem</id>
    <version>1.0.0.8</version>
    <title>IX Framework Project System</title>
    <authors>Adrian Mos</authors>
    <owners>Adrian Mos</owners>
    <developmentDependency>true</developmentDependency>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/adimosh/IX.Framework/</projectUrl>
    <description>Changed JetBrains annotations to exclude nothing.</description>
    <copyright>Adrian Mos, 2016</copyright>
