<?xml version="1.0"?>
<package >
    <metadata minClientVersion="4.0">
		<id>IX.Framework.ProjectSystem</id>
		<version>1.0.0.5</version>
		<title>IX Framework Project System</title>
		<authors>Adrian Mos</authors>
		<owners>Adrian Mos</owners>
		<developmentDependency>true</developmentDependency>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/adimosh/IX.Framework/</projectUrl>
		<description>Changed JetBrains annotations to exclude nothing.</description>
		<copyright>Adrian Mos, 2016</copyright>
		<releaseNotes>Removed problematic JetBrains reference and left it as an optional inclusion.</releaseNotes>

		<dependencies>
			<dependency id="Microsoft.SourceLink.GitHub" version="1.0.0-beta2-19270-01" />
			<dependency id="JetBrains.Annotations" version="2019.1.3" />
			<dependency id="StyleCop.Analyzers" version="1.1.118" />
			<dependency id="Microsoft.DotNet.Analyzers.Compatibility" version="0.2.12-alpha" />
			<dependency id="IDisposableAnalyzers" version="2.1.2" />
			<dependency id="ErrorProne.NET.CoreAnalyzers" version="0.2.0-beta.7" />
			<dependency id="ErrorProne.Net.Structs" version="0.2.0-beta.7" />
			<dependency id="ClrHeapAllocationAnalyzer" version="1.0.0.9" />
			<dependency id="System.Diagnostics.Tools" version="4.3.0" />
		</dependencies>

		<contentFiles>
			<files include="**/*.cs" buildAction="Compile" />
		</contentFiles>
    </metadata>
	
    <files>
        <!-- Props and Targets -->
        <file src="build\**" target="build" />
        <file src="buildMultiTargeting\**" target="buildMultiTargeting" />

        <!-- Content files -->
        <file src="contentFiles\**" target="contentFiles" />

        <!-- Readme -->
        <file src="readme.txt" target="" />
    </files>
</package>